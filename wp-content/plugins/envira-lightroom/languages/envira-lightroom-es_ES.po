msgid "No data supplied"
msgstr "No se han suministrado datos"

#: includes/global/v1/class-wp-json-media.php:284
msgid "No Content-Type supplied"
msgstr "No se ha suministrado el tipo de contenido"

#: includes/global/v1/class-wp-json-media.php:288
msgid "No Content-Disposition supplied"
msgstr "No se ha sumistrado la disposición del contenido"

#: includes/global/v1/class-wp-json-media.php:307
msgid "Invalid Content-Disposition supplied"
msgstr "No es valida la disposición del contenido que ha sumistrado"

#: includes/global/v1/class-wp-json-media.php:315
